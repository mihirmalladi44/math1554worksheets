                    <cell><b><p><m>-1</m></p></b></cell>
                    <cell><b><p><m>0</m></p></b></cell>
                    <cell><b><p><m>2</m></p></b></cell>
                    <cell><b><p><m>1</m></p></b></cell>
                </row>
                <row>
                    <cell><b><p><m>y</m></p></b></cell>
                    <cell><b><p><m>2</m></p></b></cell>
                    <cell><b><p><m>1</m></p></b></cell>
                    <cell><b><p><m>0</m></p></b></cell>
